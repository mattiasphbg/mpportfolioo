datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}
generator client {
  provider = "prisma-client-js"
}


model Portfolio_Project {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  title String
  description String
  img String?
}

model User {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  img String?
  role String[]
}

